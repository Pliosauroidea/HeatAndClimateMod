{
    "multipart": [
        {   "apply": { "model": "dcs_climate:build/table/dcs_smalltable_wood_core" }},
        {   "when": { "north": "true"},
            "apply": { "model": "dcs_climate:build/table/dcs_smalltable_wood_n" }},
            {   "when": { "south": "true"},
            "apply": { "model": "dcs_climate:build/table/dcs_smalltable_wood_s" }},
            {   "when": { "east": "true"},
            "apply": { "model": "dcs_climate:build/table/dcs_smalltable_wood_e" }},
            {   "when": { "west": "true"},
            "apply": { "model": "dcs_climate:build/table/dcs_smalltable_wood_w" }},
            {   "when": { "east": "false", "north": "false" },
            "apply": { "model": "dcs_climate:build/table/dcs_smalltable_wood_ne" }},
            {   "when": { "east": "false", "south": "false" },
            "apply": { "model": "dcs_climate:build/table/dcs_smalltable_wood_se" }},
            {   "when": { "west": "false", "north": "false" },
            "apply": { "model": "dcs_climate:build/table/dcs_smalltable_wood_nw" }},
            {   "when": { "west": "false", "south": "false" },
            "apply": { "model": "dcs_climate:build/table/dcs_smalltable_wood_sw" }}
    ]
}
