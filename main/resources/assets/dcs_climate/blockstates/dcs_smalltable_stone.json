{
    "multipart": [
        {   "apply": { "model": "dcs_climate:build/table/dcs_smalltable_stone_core" }},
        {   "when": { "north": "true"},
            "apply": { "model": "dcs_climate:build/table/dcs_smalltable_stone_n" }},
            {   "when": { "south": "true"},
            "apply": { "model": "dcs_climate:build/table/dcs_smalltable_stone_s" }},
            {   "when": { "east": "true"},
            "apply": { "model": "dcs_climate:build/table/dcs_smalltable_stone_e" }},
            {   "when": { "west": "true"},
            "apply": { "model": "dcs_climate:build/table/dcs_smalltable_stone_w" }},
            {   "when": { "east": "false", "north": "false" },
            "apply": { "model": "dcs_climate:build/table/dcs_smalltable_stone_ne" }},
            {   "when": { "east": "false", "south": "false" },
            "apply": { "model": "dcs_climate:build/table/dcs_smalltable_stone_se" }},
            {   "when": { "west": "false", "north": "false" },
            "apply": { "model": "dcs_climate:build/table/dcs_smalltable_stone_nw" }},
            {   "when": { "west": "false", "south": "false" },
            "apply": { "model": "dcs_climate:build/table/dcs_smalltable_stone_sw" }}
    ]
}
